{
  "extends": ["prettier", "eslint:recommended"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": true,
    "ecmaVersion": 6
  }
}
